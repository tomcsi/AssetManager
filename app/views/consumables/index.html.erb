  <h3>Consumables</h3>
  <%= link_to "New consumable", new_consumable_path %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Category</th>
        <th>QTY</th>
        <th>Responsible</th>
        <th width="80px"></th>
      </tr>
    </thead>
    <tbody>
      <% @consumables.each do |consumable| %>
      <tr>
      <td><%= consumable.name %></td>
      <td><%= consumable.category.name %></td>
      <td><%= consumable.qty.to_s %> db</td>
      <td><%= consumable.responsible.name %></td>
      <td>
        <%= link_to edit_consumable_path(consumable) do %>
          <span class="glyphicon glyphicon-pencil" aria-hidden="true">&nbsp;</span>
        <% end %>
        <%= link_to consumable_path(consumable), method: :delete do %>
          <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        <% end %>
      </td>
      </tr>
      <% end %>
    </tbody>
  </table>