  <%= form_for @consumable do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :qty %>
    <%= f.number_field :qty, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :category_id %>
    <%= f.select :category_id, Category.all.collect { |p| [p.name, p.id] }, {}, { class: "form-control" } %>
  </div>
  <div class="form-group">
    <%= f.label :responsible_id %>
    <%= f.select :responsible_id, Person.all.collect { |p| [p.name, p.id] }, {}, { class: "form-control" } %>
  </div>
  <%= link_to "Back", consumables_path, class: "btn btn-default" %>
  <%= f.submit class: "btn btn-success" %>
  <% end %>
